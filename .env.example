# 生产环境配置示例
# 请复制此文件为 .env.production 并填入真实的密钥

# 豆包火山方舟语音合成配置
VOLCENGINE_TTS_APP_ID=你的App_ID
VOLCENGINE_TTS_ACCESS_TOKEN=你的Access_Token
VOLCENGINE_TTS_CLUSTER=你的Cluster
VOLCENGINE_TTS_VOICE_TYPE=你的默认音色
VOLCENGINE_TTS_SECRET_KEY=你的Secret_Key

# OpenRouter AI配置
OPENROUTER_API_KEY=你的OpenRouter_API_Key
OPENROUTER_MODEL=qwen/qwen3-32b:free

# 服务器配置
PORT=3000
NODE_ENV=production

# 模型配置
DEFAULT_VOICE_TYPE=zh_female_linjia_mars_bigtts
DEFAULT_SPEED=1.0
DEFAULT_VOLUME=1.0
